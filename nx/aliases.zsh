## NX ##
alias nxbaa="pnpm nx run-many --target=build --all"
alias nxb="pnpm nx build"
alias nxba="pnpm nx run-many --target=build --parallel=true --maxParallel=8 --all=true --skip-nx-cache=true"
alias nxta="pnpm nx run-many --target=test --parallel=true --maxParallel=8 --all=true --skip-nx-cache=true"
alias nxt="pnpm nx run-many --target=test --all"
alias nxbta="nxba && nxta"
alias nxaddlib="pnpm nx g @nrwl/workspace:lib"
alias nxml="pnpm nx migrate latest"
alias nxrm="pnpm nx migrate --run-migrations"
